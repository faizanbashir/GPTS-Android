<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/nav_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:weightSum="100"
    android:background="#fff"
    android:paddingTop="?android:attr/actionBarSize">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="45"
        android:padding="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <android.support.percent.PercentRelativeLayout
            xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_width="fill_parent"
            android:layout_height="fill_parent">

            <ImageView
                android:id="@+id/partImage"
                android:background="#fff"
                app:layout_marginBottomPercent="0%"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="65%"
                app:layout_heightPercent="100%"
                android:layout_centerHorizontal="true"/>

        </android.support.percent.PercentRelativeLayout>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="14"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <TextView
            android:id="@+id/companyName"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="20sp"/>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:paddingTop="2.5dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <TextView
            android:text="GP NUMBER"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:textColor="#690"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/gpno"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:layout_marginTop="1dp"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="13sp"/>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:paddingTop="2.5dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <TextView
            android:text="OEM NUMBER"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:textColor="#690"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/partNumber"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:layout_marginTop="1dp"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="13sp"/>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="9"
        android:paddingTop="2.5dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <TextView
            android:text="APPLICATION"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:textColor="#690"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/application"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:layout_marginTop="1dp"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="13sp"/>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:layout_weight="13"
        android:paddingTop="2.5dp"
        android:paddingRight="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:layout_marginLeft="0dp"
        android:background="#fff">

        <TextView
            android:text="MODEL"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:textColor="#690"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="14sp"/>

        <TextView
            android:id="@+id/model"
            android:textStyle="normal"
            android:shadowColor="@color/text_shadow_white"
            android:shadowDx="-2"
            android:layout_marginTop="1dp"
            android:shadowDy="-2"
            android:shadowRadius="1"
            android:background="#fff"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="13sp"/>

    </LinearLayout>

    <View
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:background="#c0c0c0"/>

</LinearLayout>

    <android.support.percent.PercentRelativeLayout
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/nav_left"
        android:layout_gravity="left"
        android:background="@drawable/nave"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:background="@android:color/transparent"
            android:layout_alignParentLeft="true"
            android:id="@+id/nav_home"
            android:layout_width="95dp"
            android:layout_height="35dp"
            app:layout_widthPercent="35%"
            app:layout_heightPercent="5%"
            app:layout_marginLeftPercent="2%"
            app:layout_marginTopPercent="15.4%"/>

        <android.support.percent.PercentRelativeLayout
            xmlns:app="http://schemas.android.com/apk/res-auto"
            android:layout_below="@+id/nav_home"
            android:id="@+id/lay_min"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_marginTopPercent="0.2%"
            app:layout_marginLeftPercent="2%">

            <ImageButton
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_catalogue"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_catalogue"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_upcomingevents"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_upcomingevents"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_newlaunch"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_newlaunch"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_turbofailure"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_turbofailure"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_aboutus"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_aboutus"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_enquiry"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_enquiry"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_faq"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <ImageButton
                android:layout_below="@+id/nav_faq"
                android:background="@android:color/transparent"
                android:layout_alignParentLeft="true"
                android:id="@+id/nav_feedback"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="7.8%"/>

            <View
                android:id="@+id/lazy_view"
                android:layout_below="@id/nav_feedback"
                android:background="@android:color/transparent"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_widthPercent="77%"
                app:layout_heightPercent="40%"/>

        </android.support.percent.PercentRelativeLayout>

    </android.support.percent.PercentRelativeLayout>

</android.support.v4.widget.DrawerLayout>